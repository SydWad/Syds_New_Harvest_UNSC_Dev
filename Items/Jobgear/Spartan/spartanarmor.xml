    <ItemContainer capacity="0" hideitems="true" containedstateindicatorslot="0" containedstateindicatorstyle="battery">
      <SlotIcon slotindex="0" texture="%ModDir%/UI/Container_Icons.png" sourcerect="64,0,64,64" origin="0.5,0.5" />
      <SlotIcon slotindex="1" texture="%ModDir%/UI/Container_Icons.png" sourcerect="256,0,64,64" origin="0.5,0.5" />
      <SlotIcon slotindex="2" texture="%ModDir%/UI/Container_Icons.png" sourcerect="128,0,64,64" origin="0.5,0.5" />
      <!-- When power source is contained -->
      <SubContainer capacity="1" maxstacksize="1">
        <!-- 60 minutes of use -->
        <Containable identifiers="nh_fusion_power">
          <StatusEffect type="OnWearing" targettype="Contained" targetitemslot="0" condition="47.2222222222" />
        </Containable>
        <!-- 1 minute of use -->
        <Containable identifiers="alienpowercell">
          <StatusEffect type="OnWearing" targettype="Contained" targetitemslot="0" condition="48.3333333333" />
        </Containable>
        <Containable identifiers="nh_fusion_power,mobilebattery,alienpowercell">
          <StatusEffect type="OnWearing" targettype="Contained" targetitemslot="0" condition="-50" />
          <StatusEffect type="OnWearing" targettype="This,Character" voltage="1" SpeedMultiplier="1.85" setvalue="true" />
          <StatValue stattype="WalkingSpeed" value="0.85" />
          <StatusEffect type="OnWearing" targettype="This,Character" setvalue="true" comparison="and">
            <Conditional nh_energy_shields="lt 1" />
            <RequiredItem identifier="nh_shieldmodule" type="Contained" />
            <Affliction identifier="nh_energy_shields" amount="1" />
          </StatusEffect>
          <!-- Play recharge sound on loop when shields increasing -->
          <StatusEffect type="OnWearing" targettype="This,Character" comparison="and">
            <Conditional IsLocalPlayer="True" />
            <Conditional nh_energy_shields="gt 2" />
            <Conditional nh_energy_shield_delay_short="lteq 0" />
            <Conditional nh_energy_shield_delay_long="lteq 0" />
            <RequiredItem identifier="nh_shieldmodule" type="Contained" />
            <Sound file="%ModDir%/Afflictions/spartan_shield_recharge.ogg" volume="1.25" loop="true" />
          </StatusEffect>
        </Containable>
      </SubContainer>
      <!-- When medicine is contained -->
      <SubContainer capacity="1" maxstacksize="1" autoinject="true">
        <Containable identifiers="medical" />
      </SubContainer>
      <SubContainer capacity="1" maxstacksize="1">
        <!-- When shield module is contained -->
        <Containable identifiers="nh_shieldmodule" />
        <!-- When extra armor is contained -->
        <Containable identifiers="nh_armormodule">
          <!-- Hate it but i had to make three in order to have three levels of protection for different types -->
          <StatusEffect type="OnWearing" targettype="Character" setvalue="true">
            <Affliction identifier="nh_extra_armor_1" amount="100" />
            <Affliction identifier="nh_extra_armor_2" amount="100" />
            <Affliction identifier="nh_extra_armor_3" amount="100" />
          </StatusEffect>
        </Containable>
      </SubContainer>
    </ItemContainer>
